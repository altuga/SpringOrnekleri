<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
     http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="voltran" class="com.kodcu.main.Voltran">
        <constructor-arg ref="gucKaynagi"/>
    </bean>

    <bean id="voltran2" class="com.kodcu.main.Voltran">
        <constructor-arg ref="silah"/>
    </bean>


    <bean id="gucKaynagi"
          class="com.kodcu.main.GunesEnerjisi"/>

    <bean id="silah"
          class="com.kodcu.main.LaserSilahi"/>

</beans>
